{
  "name": "beygla",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --coverage",
    "build": "npm run build:prepare && npm run build:bundle && npm run build:copy-files",
    "build:prepare": "npm run build:remove-old-build",
    "build:remove-old-build": "rm -rf dist/ && rm -rf out/",
    "build:bundle": "rollup -c --bundleConfigAsCjs",
    "build:copy-files": "cp README.md package.json dist/ && cp dist/read/beygla.d.ts dist/ && rm -rf dist/read/ && rm -rf dist/constants.d.ts",
    "filter-names": "ts-node -T -O '{\"module\":\"commonjs\"}' ./scripts/filter-names.ts",
    "process-names": "ts-node -T -O '{\"module\":\"commonjs\"}' ./scripts/process-names.ts",
    "group-names": "ts-node -T -O '{\"module\":\"commonjs\"}' ./scripts/group-names.ts",
    "sankey-diagram": "ts-node -T -O '{\"module\":\"commonjs\"}' ./scripts/sankey-diagram.ts"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "axios": "^1.1.3",
    "gray-matter": "^4.0.3",
    "jest": "^29.3.1",
    "next": "13.0.1",
    "next-mdx-remote": "^4.1.0",
    "node-fetch": "^3.2.10",
    "prism-react-renderer": "^1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.55.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jest": "^29.2.2",
    "rollup": "^3.3.0",
    "ts-node": "^10.9.1"
  }
}
